	<style type="text/css">
		.flex {
			display: flex;
			/*justify-content: space-between;*/
		}
		#title {
			width: 100%;
		}
		.dull {
			color: lightgrey;
			background: grey;
			border: lightgrey;
			cursor: default;
		}
		.dull:hover{
			color: lightgrey;
			background: grey;
			border: lightgrey;
			cursor: default;
		}
		.size {
			text-overflow: ellipsis;
			overflow: hidden;
			width: 90%;
		}
		#ticles {
			width: 20%;
		}
		#cancel {
			background: red;
